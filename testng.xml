<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Sports Direct Automation Suite" parallel="tests" preserve-order="true">
    <listeners>
        <listener class-name="com.smava.utils.WebDriverListener"></listener>
    </listeners>
    <test name="Smoke">
        <parameter name="browserName" value="chrome"></parameter>
        <classes>
            <class name="com.smava.tests.LoanSelectionTest"/>
            <class name="com.smava.tests.LoginTest"/>
        </classes>
    </test>
</suite>